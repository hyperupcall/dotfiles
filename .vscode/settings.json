{
	"shellcheck.ignorePatterns": {
		"**/*.z*": true,
		"**/.tcshrc": true,
		"**/.cshrc": true,
		"**/.git": true
	},
	"files.associations": {
		"**/i3/**": "i3",
		"**/polybar/**": "ini"
	},
	"css.validate": false,
	"editor.codeActionsOnSave": {
		"source.fixAll": true
	},
	"esbonio.server.enabled": true,
	"search.exclude": {
		"**/.git/**": true,
		"**/.home/**": true,
		"**/.repos/**": true,
		"**/.usr/**": true
	},
	"commentAnchors.workspace.excludeFiles": "**/{.dotmgr,.git,.home,.repos,.usr,Code/User/settings.json}/**",
	"esbonio.sphinx.confDir": ""
}
